<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                              https://maven.apache.org/xsd/settings-1.0.0.xsd">
    <localRepository>
        ${user.home}/.m2/repository/.
    </localRepository>
    <interactiveMode/>
    <usePluginRegistry/>
    <offline/>
    <pluginGroups/>
    <servers/>
    <mirrors/>
    <proxies/>
    <profiles>
        <profile>
            <id>deploy-build</id>
            <activation>
                <activeByDefault>true</activeByDefault>
                <jdk>1.8</jdk>
            </activation>
            <properties>
                <user.deploy>/Development/Dropbox/eCIS_Development/eCIS-LIB/deploy</user.deploy>
                <user.localDirectoryPath>/Development/Dropbox/eCIS_Development/eCIS-LIB/local-maven-repo/</user.localDirectoryPath>
                <user.localMavenRepo>file:///C:/Development/Dropbox/eCIS_Development/eCIS-LIB/local-maven-repo/</user.localMavenRepo>
            </properties>
        </profile>
    </profiles>
    <activeProfiles/>
</settings>

